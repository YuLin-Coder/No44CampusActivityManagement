	</select>
	
	<!-- 
	 1.方法描述：查询分页(自定义)
	 2.参数：实体类
	-->
	<select id="findByEntitySqlId" resultMap="ResultMapRoleAction" parameterType="com.my.xymh.entity.RoleAction">
		select <include refid="RoleAction_field" />
 		from role_action where 1=1
 		<include refid="RoleAction_where" /> 
