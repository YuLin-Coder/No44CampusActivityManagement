	<select id="findBySqlId_count" resultMap="ResultMapRoleAction" parameterType="map">
		select count(*)
 		from role_action where 1=1
 		<include refid="RoleAction_where" /> 
	</select>
	
	<!-- 
	 1.方法描述：批量插入
	 2.参数：集合/数组
	-->
	<insert id="insertBatch" parameterType="java.util.List">
        INSERT INTO role_action (
        <include refid="RoleAction_field" />
        ) VALUES 
        <foreach collection="list" item="item" index="index"
			separator=",">
			(
			#{item.id},
			
			#{item.actionId},
			
			#{item.roleId}
			
			)
		</foreach>       
        
	</insert>
    
    <!-- 
	 1.方法描述：批量修改
	 2.参数：集合/数组
	-->
	<update id="updateBatch" parameterType="java.util.List">
		<foreach collection="list" item="item" index="index"
			separator=";">
        UPDATE 
        role_action 
        <set>
		    <if test="item.actionId != null" >
	        actionId = #{item.actionId},
	        </if>
		    <if test="item.roleId != null" >
	        roleId = #{item.roleId}
	        </if>
	    </set>
        WHERE 
        	1=1 
        </foreach>    
	</update>

</mapper>

