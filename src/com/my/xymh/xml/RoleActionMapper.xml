        	<include refid="RoleAction_where" />
	</select>
	
	<!-- 
	 1.方法描述：用于select查询所有
	 2.参数：map
	-->
	<select id="list" resultMap="ResultMapRoleAction" parameterType="map">
		select <include refid="RoleAction_field" />
 		from role_action where 1=1
 		<include refid="RoleAction_where" /> 
	</select>
	<select id="selectByroleId" resultMap="ResultMapRoleAction" parameterType="int">
		select <include refid="RoleAction_field" />
 		from role_action where 1=1 and roleId = #{id}
	</select>
	
	<!-- 
	 1.方法描述：用于select查询所有(自定义)
	 2.参数：map
	-->
	<select id="listBySqlId" resultMap="ResultMapRoleAction" parameterType="map">
		select <include refid="RoleAction_field" />
 		from role_action where 1=1
 		<include refid="RoleAction_where" /> 
	</select>
	
	<!-- 
	 1.方法描述：用于select查询所有
	 2.参数：无参数
	-->
	<select id="listAll" resultMap="ResultMapRoleAction">
		select <include refid="RoleAction_field" />
 		from role_action 
	</select>
	
	
	<!-- 
	 1.方法描述：用于select查询所有(自定义)
	 2.参数：实体类
	-->
	<select id="listAllByEntity" resultMap="ResultMapRoleAction" parameterType="com.my.xymh.entity.RoleAction">
		select <include refid="RoleAction_field" />
 		from role_action where 1=1
 		<include refid="RoleAction_where" /> 
	</select>
	
	
	
	<!-- 
