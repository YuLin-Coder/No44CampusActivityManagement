	 2.参数：map
	-->
	<select id="findByMap" resultMap="ResultMapRoleAction" parameterType="map">
		select <include refid="RoleAction_field" />
 		from role_action where 1=1
 		<include refid="RoleAction_where" /> 
 		order by id desc
	</select>
	
	<!-- 
	 1.方法描述：查询分页(自定义)
	 2.参数：map
	-->
	<select id="findByMapSqlId" resultMap="ResultMapRoleAction" parameterType="map">
		select <include refid="RoleAction_field" />
 		from role_action where 1=1
 		<include refid="RoleAction_where" /> 
 		order by id desc
	</select>
	
	
	<!-- 
	 1.方法描述：查询分页 非插件
	 2.参数：map
	-->
	<select id="find" resultMap="ResultMapRoleAction" parameterType="map">
		select <include refid="RoleAction_field" />
 		from role_action where 1=1
 		<include refid="RoleAction_where" /> 
 		order by id desc limit #{pageOffset},#{pageSize}
	</select>
	
	<!-- 
	 1.方法描述：查询分页  非插件    总数
	 2.参数：map
	-->
	<select id="find_count" resultMap="ResultMapRoleAction" parameterType="map">
		select count(*)
 		from role_action where 1=1
